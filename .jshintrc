{
	// Enforcing options
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "es3": false,
	"es5": true,
  "forin": true,
  "freeze": true,
	"funcscope": true,
	"futurehostile": true,
	"iterator": true,
  "latedef": true,
	"maxcomplexity": true,
	"maxdepth": 2,
	"maxerr": 50,
	"maxparams": 5,
	"maxstatements": 4,
  "noarg": true,
	"nocomma": true,
  "nonbsp": true,
  "nonew": true,
	"notypeof": true,
  "quotmark": "single",
	"shadow": false,
	"singleGroups": true,
	"strict": true,
  "undef": true,
  "unused": true,
	"vartmt": true,

  // Relaxing options
  "asi": false,
  "boss": false,
	"debug": false,
	"elision": false,
  "eqnull": true,
	"esnext": false,
	"evil": false,
	"expr": false,
	"globalstrict": false,
	"lastsemic": false,
	"loopfunc": false,
	"moz": false,
	"noyield": false,
  "plusplus": true,
	"proto": false,
	"scripturl": false,
  "supernew": false,
	"validthis": false,
	"withstmt": false,

  "globals": {
  }
}